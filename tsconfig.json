{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "noFallthroughCasesInSwitch": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "preserveConstEnums": true,
        "sourceMap": true
    },
    "compileOnSave": true,
    "buildOnSave": false,
    "exclude": ["node_modules", "typings"],
    "atom": {
        "rewriteTsconfig": false
    }
}
